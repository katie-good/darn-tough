/* Expander Style When Expander is Narrow */

.narrow-expander table {
    overflow-x: scroll;
}

.narrow-expander tr th:first-child {
    position: sticky;
    left: 0;
    z-index: 1;
    min-width: 125px;
    max-width: 125px;
    width: 125px;
    border-left: 1px solid #e8e8e1;
}

.narrow-expander tr td:first-child {
    position: sticky;
    left: 0;
    z-index: 1;
    min-width: 125px;
    max-width: 125px;
    width: 125px;
    border-left: 1px solid #e8e8e1;
}

.narrow-expander tr.visible td:first-child::after {
       content: "";
       position: absolute;
       top: 0;
       right: -24px;
       width: 24px;
       height: 100%;
       opacity: 1;
       -webkit-transition: opacity 250ms;
       transition: opacity 250ms;
       background: linear-gradient(to right,rgba(17,17,17,0.12) 0%,rgba(255,255,255,0) 100%);
       pointer-events: none;

}

.narrow-expander tr.visible th:first-child::after {
     content: "";
     position: absolute;
     top: 0;
     right: -24px;
     width: 24px;
     height: 100%;
     opacity: 1;
     -webkit-transition: opacity 250ms;
     transition: opacity 250ms;
     background: linear-gradient(to right,rgba(17,17,17,0.12) 0%,rgba(255,255,255,0) 100%);
     pointer-events: none;

}

.overflow {
        background: linear-gradient(to left,rgba(17,17,17,0.12) 0%,rgba(255,255,255,0) 100%);
        opacity: 0;
        -webkit-transition: opacity 250ms;
        transition: opacity 250ms;
        width: 24px;
        margin-left: -24px;
        z-index: 1;
        pointer-events: none;
        position: absolute;
        top: 0;
        right: 0;
        height: calc(100% - 35px);
        right: 20px;
}

.overflow.visible {
    opacity: 1;
}

